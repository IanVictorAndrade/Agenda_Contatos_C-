@model ContatoModel
@{
    ViewData["Title"] = "Listagem de Contatos";
}

<h1 class="display-4">Listagem de Contatos</h1>

<table class="table">
    <thead>
    <tr>
        <th>Nome</th>
        <th>Email</th>
        <th>Telefone</th>
    </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Nome</td>
            <td>@Model.Email</td>
            <td>@Model.Telefone</td>
        </tr>
    </tbody>
</table>